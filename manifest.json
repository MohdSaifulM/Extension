{
  "name": "LifeKeeper",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "notifications"
  ],
  "web_accessible_resources": [
    {
      "resources": ["build/static/js/main.js"],
      "matches": ["<all_urls>"]
    }
  ]
}